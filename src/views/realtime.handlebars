<div id="contenedorProductos"></div>

<h1>Registro de Productos</h1>

<form>
    <label for="title">Título:</label>
    <input type="text" id="title" placeholder="Ingrese el título" required>

    <label for="description">Descripción:</label>
    <input type="text" id="description" placeholder="Ingrese la descripción" required>

    <label for="price">Precio:</label>
    <input type="number" id="price" placeholder="Ingrese el precio" required>

    <label for="img">Imagen:</label>
    <input type="text" id="img" value="Sin Imagen">

    <label for="code">Código:</label>
    <input type="text" id="code" placeholder="Ingrese el código" required>

    <label for="stock">Stock:</label>
    <input type="number" id="stock" placeholder="Ingrese el stock" required>

    <label for="category">Categoría:</label>
    <input type="text" id="category" placeholder="Ingrese la categoría" required>

    <label for="status">Estado:</label>
    <select id="status">
        <option value="true">Activo</option>
        <option value="false">Inactivo</option>
    </select>

    <button type="button" id="btnEnviar">Enviar</button>
</form>

<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.7.2/socket.io.min.js"></script>
<script src="/js/index.js"></script>